{
  "version": 2,
  "name": "cultureon-analytics",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://cultureon-backend.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://cultureon-backend.up.railway.app/api",
    "REACT_APP_SOCKET_URL": "https://cultureon-backend.up.railway.app"
  },
  "build": {
    "env": {
      "CI": "false"
    }
  }
}